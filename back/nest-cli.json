{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/back/src",
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/back/tsconfig.app.json"
    },
    "monorepo": true,
    "root": "apps/back",
    "projects": {
        "back": {
            "type": "application",
            "root": "apps/back",
            "entryFile": "main",
            "sourceRoot": "apps/back/src",
            "compilerOptions": {
                "tsConfigPath": "apps/back/tsconfig.app.json"
            }
        },
        "thread": {
            "type": "application",
            "root": "apps/thread",
            "entryFile": "main",
            "sourceRoot": "apps/thread/src",
            "compilerOptions": {
                "tsConfigPath": "apps/thread/tsconfig.app.json"
            }
        },
        "sender": {
            "type": "application",
            "root": "apps/sender",
            "entryFile": "main",
            "sourceRoot": "apps/sender/src",
            "compilerOptions": {
                "tsConfigPath": "apps/sender/tsconfig.app.json"
            }
        }
    }
}
